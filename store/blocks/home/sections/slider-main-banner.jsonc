          "link": {
            "default": "502?PS=24&map=c,productClusterIds&O=OrderByReleaseDateDESC",
            "title": "Link",
            "url": "502?PS=24&map=c,productClusterIds&O=OrderByReleaseDateDESC"
          }
        }
      ]
    }
  },
  "slider-layout#banner-principal": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "fullWidth": true,
      "infinite": true,
      "autoplay": {
        "timeout": 10000,
        "stopOnHover": false
      },
      "blockClass": "main-banner",
      "showNavigationArrows": "never",
      "arrowSize": 35,
      "showPaginationDots": "always"
    }
  }
}
