{
  "flex-layout.row#mobile-menu-item-hombre": {
    "children": [
      "rich-text#item-menu-category-hombre",
      "drawer#item-menu-category-hombre"
    ],
    "props": {
      "blockClass": "mobile-menu-item"
    }
  },
  "rich-text#item-menu-category-hombre": {
    "props": {
      "text": "Hombre",
      "blockClass": [
        "item-menu-principal",
        "item-menu-principal-categorias",
        "icon"
      ]
    }
  },
  "drawer#item-menu-category-hombre": {
    "children": [
      "menu#menu-hombre"
    ],
    "props": {
      "blockClass": [
        "item-menu-principal",
        "item-menu-principal-categorias"
      ],
      "slideDirection": "leftToRight",
      "maxWidth": 320
    }
  },
  "menu#menu-hombre": {
    "children": [
      "flex-layout.row#submenu-close",
      "flex-layout.row#mobile-submenu-elements-hombre",
      "flex-layout.row#submenu-items"
    ],
    "props": {
      "orientation": "vertical",
      "blockClass": "mobile-submenu-container"
    }
  },
  "flex-layout.row#mobile-submenu-elements-hombre": {
    "children": [
      "vtex.menu@2.x:menu#hombre-col2",
      "flex-layout.row#submenu-title-hombre",
      "vtex.menu@2.x:menu#hombre-col1"
    ],
    "props": {
      "orientation": "vertical",
      "blockClass": "mobile-submenu-elements"
    }
  }
}
